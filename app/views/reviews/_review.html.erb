<p><h4><%= t("titolo") +review.title %></h4></p>
<p><%= "Rating: "+review.rating.to_s %></p>
<p><%= "Commento: "+review.comment %></p>
<p><%= "Negozio: "+review.negozio %></p>


<% if current_user==@user  || current_user.moderator? || current_user.admin?%>
<%= link_to t("modifica") , edit_user_review_path(review.user_id, review) %>
<%= link_to t("elimina") , user_review_path(review.user_id, review), data: {turbo_method: :delete, turbo_confirm:"Are you sure?"}%>
<%end %>
